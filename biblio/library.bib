isbn = {9566658761},
issn = {14203049},
journal = {Molecules},
keywords = {Density functional theory,Electron localization function,Graphene,Li,Na,Quantum theory of atoms in molecules},
number = {4},
pages = {1--28},
pmid = {30791506},
title = {{Li and Na adsorption on graphene and graphene oxide examined by density functional theory, quantum theory of atoms in molecules, and electron localization function}},
volume = {24},
year = {2019}
