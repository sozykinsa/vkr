@article{Politzer2017,
abstract = {After a brief discussion of the $\sigma$-hole concept and the significance of molecular electrostatic potentials in noncovalent interactions, we draw attention to some common misconceptions that are encountered in that context: (1) Since the electrostatic potential reflects the contributions of both the nuclei and the electrons, it cannot be assumed that negative potentials correspond to “electron-rich” regions and positive potentials to “electron-poor” ones; (2) The electrostatic potential in a given region is determined not only by the electrons and nuclei in that region, but also by those in other portions of the molecule, especially neighboring ones; (3) A $\sigma$-hole is a region of lower electronic density on the extension of a covalent bond, not an electrostatic potential; (4) Noncovalent interactions are between positive and negative regions, which are not necessarily associated with specific atoms, so that “close contacts” between atoms do not always indicate the actual interactions.},
author = {Politzer, Peter and Murray, Jane S.},
doi = {10.3390/cryst7070212},
file = {:D\:/YandexDisk/YandexDisk/Mendeley_db/$\sigma$-Hole interactions Perspectives and misconceptions - 2017 - Politzer, Murray.pdf:pdf},
issn = {20734352},
journal = {Crystals},
keywords = {Close contacts,Electrostatic potential,Noncovalent interactions,$\sigma$-holes},
pages = {212},
title = {{$\sigma$-Hole interactions: Perspectives and misconceptions}},
volume = {7},
year = {2017}
}
