pmid = {24071824},
title = {{Direct measurement of the absolute absorption spectrum of individual semiconducting single-wall carbon nanotubes.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/24071824},
volume = {4},
year = {2013}
}
