isbn = {0021998317741},
issn = {1530793X},
journal = {Journal of Composite Materials},
keywords = {Epoxy,elastic modulus,fracture resistance,graphene oxide,hardness,nano-composites,nano-indentation},
number = {16},
pages = {2179--2188},
title = {{Improved mechanical performance of bisphenol-A graphene-oxide nano-composites}},
