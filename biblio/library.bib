journal = {International Journal of Environmental Science and Technology},
keywords = {Adsorption,Biomass,Graphene oxide,Hydroxytyrosol,Olive mill wastewater},
number = {12},
pages = {4803--4814},
publisher = {Springer Berlin Heidelberg},
title = {{Investigation of graphene oxide as highly selective adsorbent in recovery of hydroxytyrosol from olive mill wastewater}},
url = {https://doi.org/10.1007/s13762-020-02813-x},
volume = {17},
year = {2020}
