issn = {00086223},
journal = {Carbon},
pages = {425--432},
title = {{Insight into modification of electrodonor properties of multiwalled carbon nanotubes via oxygen plasma: Surface functionalization versus amorphization}},
volume = {137},
year = {2018}
}
