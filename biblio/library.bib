journal = {ACS Nano},
keywords = {carbon nanotubes,electronic structure,exciton localization,oxygen doping,photoluminescence},
number = {10},
pages = {10782--10789},
pmid = {25265272},
title = {{Electronic structure and chemical nature of oxygen dopant states in carbon nanotubes}},
volume = {8},
year = {2014}
}
