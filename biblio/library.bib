issn = {00496979},
journal = {Water, Air, and Soil Pollution},
keywords = {17$\beta$-Estradiol,Adsorption energy,Adsorption kinetics,Bisphenol A,Density functional theory,Singlewalled carbon nanotubes},
month = {jul},
number = {6},
pages = {3281--3293},
publisher = {Springer},
title = {{Removal of bisphenol a and 17$\beta$-estradiol by single-walled carbon nanotubes in aqueous solution: Adsorption and molecular modeling}},
