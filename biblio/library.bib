pages = {2162},
title = {{Conducting linear chains of sulphur inside carbon nanotubes.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3717502&tool=pmcentrez&rendertype=abstract},
volume = {4},
year = {2013}
}
@article{Kasuga1999,
