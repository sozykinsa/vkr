author = {Qi, Guocheng and Rabczuk, Timon},
doi = {10.1016/j.carbon.2019.08.027},
file = {:D\:/YandexDisk/YandexDisk/Mendeley_db/Ab initio investigation of lithium adsorption on short carbon nanotubes considering effects of the tube length - 2019 - Qi, Rabczuk.pdf:pdf},
issn = {00086223},
