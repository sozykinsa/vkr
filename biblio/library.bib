@article{Mohammadzadeh2015,
abstract = {We examine the insertion of alkali and halide ions into narrow nanotubes of graphite and gold by density functional theory (DFT). For tubes with diameters less than about 10 {\AA}the optimum position of the ion is in the center of the tube. Bader analysis and an analysis of the densities of states gave contradictory results for the charge on the halide ions, but we argue on physical grounds that they carry unit negative charge. We have calculated the energies of inserting the atoms into the tubes, where they are ionized. Because of the small system size the work function of the tubes changes during this process, which makes it difficult to interpret these energies. The surrounding tubes screen the ionic charge very effectively; thereby the ion-ion interactions are strongly reduced, which explains, why narrow tubes store charge more effectively than wider one. These effects are stronger for gold than for graphite tubes. We define an effective image radius of a cylindrical tube, and calculate the image energy experienced by the ions.},
author = {Mohammadzadeh, Leila and Goduljan, Aleksej and Juarez, Fernanda and Quaino, Paola and Santos, Elizabeth and Schmickler, Wolfgang},
doi = {10.1016/j.electacta.2014.12.031},
file = {:D\:/YandexDisk/YandexDisk/Mendeley_db/Nanotubes for charge storage - Towards an atomistic model - 2015 - Mohammadzadeh et al.pdf:pdf},
issn = {00134686},
journal = {Electrochimica Acta},
keywords = {Charge storage,DFT,Image charge,Nanotubes,Screening},
pages = {11--16},
publisher = {Elsevier Ltd},
title = {{Nanotubes for charge storage - Towards an atomistic model}},
url = {http://dx.doi.org/10.1016/j.electacta.2014.12.031},
volume = {162},
year = {2015}
}
@article{Ordejon1996,
abstract = {We present a method to perform fully selfconsistent density-functional calculations, which scales linearly with the system size and which is well suited for very large systems. It uses strictly localized pseudoatomic orbitals as basis functions. The sparse Hamiltonian and overlap matrices are calculated with an $O(N)$ effort. The long range selfconsistent potential and its matrix elements are computed in a real-space grid. The other matrix elements are directly calculated and tabulated as a function of the interatomic distances. The computation of the total energy and atomic forces is also done in $O(N)$ operations using truncated, Wannier-like localized functions to describe the occupied states, and a band-energy functional which is iteratively minimized with no orthogonality constraints. We illustrate the method with several examples, including carbon and silicon supercells with up to 1000 Si atoms and supercells of $\beta$-C$_3$N$_4$. We apply the method to solve the existing controversy about the faceting of large icosahedral fullerenes by performing dynamical simulations on C$_{60}$, C$_{240}$, and C$_{540}$.},
archivePrefix = {arXiv},
arxivId = {cond-mat/9601002},
author = {Ordejon, Pablo and Artacho, Emilio and Soler, Jos{\'{e}} M.},
doi = {10.1103/PhysRevB.53.R10441},
eprint = {9601002},
file = {:D\:/YandexDisk/YandexDisk/Mendeley_db/Selfconsistent order-N density-functional calculations for very large systems - 1996 - Ordejon, Artacho, Soler.pdf:pdf},
isbn = {0163-1829},
issn = {0163-1829},
journal = {Physical Review B},
number = {16},
pages = {4},
pmid = {9982701},
primaryClass = {cond-mat},
title = {{Selfconsistent order-N density-functional calculations for very large systems}},
url = {http://arxiv.org/abs/cond-mat/9601002},
volume = {53},
