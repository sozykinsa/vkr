issn = {00086223},
journal = {Carbon},
pages = {141--159},
publisher = {Elsevier Ltd},
title = {{Accurate determination of the chiral indices of individual carbon nanotubes by combining electron diffraction and Resonant Raman spectroscopy}},
url = {http://linkinghub.elsevier.com/retrieve/pii/S0008622316310570},
