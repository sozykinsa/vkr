journal = {Optik},
keywords = {CASTEP,Co-doping,Density functional theory,Material studio,Optical properties,Single-walled carbon nanotubes},
pages = {267--272},
publisher = {Elsevier GmbH.},
title = {{Static refractive index engineering of a singlewalled carbon nanotube through co-doping: A theoretical study}},
url = {http://dx.doi.org/10.1016/j.ijleo.2016.11.095},
volume = {131},
