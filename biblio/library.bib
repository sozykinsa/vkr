issn = {15205002},
journal = {Chemistry of Materials},
language = {en},
pages = {471--482},
title = {{Enhancing the plasmon resonance absorption of multibranched gold nanoparticles in the near-infrared region for photothermal cancer therapy: theoretical predictions and experi- mental verification}},
volume = {31},
year = {2019}
}
